<%= form_for(@program) do |f| %>
  <%= errors_for @program %>


  <div>
    <%= f.label :version %> 
  </div>
  <div>
    <%= text_field :program, :version, :size => "100" %> 
  </div>
  <div class="actions">
    <%= f.submit "update version string" %>
  </div>

<% end %>

<%= render 'programs/program_matrix', :program => @program, :editable => true %>




