<table class="tablelist">
  <tr>
    <th></th>
    <th></th>
<% if @voting_active  %>
    <th> Votée ? </th>
<% end %>
    <th> Session </th>
  </tr>

<% sessions.each_with_index do |session, i| %>
  <tr>
    <td> <%= i+1 %> </td>
    <td> 
      <div class="session_status">
        <%= session.status (@previous_login_time)%>
      </div>
    </td>
<% if @voting_active  %>
    <td> <% if session.presenter_has_voted_for?(@current_account.presenter.id) %> OUI  <% end %> </td>
<% end %>
    <td class="<%= session.topic %>"> <%= link_to w(session.title) , session %> 
      <br/> <i> by <%= session_presenter_names(session) %> </i>
  </tr>
<% end %>
</table>
