
<table class="tablelist">
  <tr>
    <th></th>
    <th> <%= "Session" %>  </th>
    <th> <%= "Voted by" %> </th>
    <th> <%= "Created" %>  </th>
  </tr>

<% votes.each_with_index do |vote, i| %>
  <tr>
    <td> <%= i+1 %> </td>
    <td> <%= link_to vote.session.title , vote.session %></td>
    <td> <%= link_to vote.presenter.name, vote.presenter %> </td>
    <td class="nowrap" ><%= l vote.created_at %> </td>
  </tr>
<% end %>
</table>

