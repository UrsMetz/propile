<table class="tablelist">
  <tr>
    <th></th>
    <th> <%= "Session" %>  </th>
    <th> <%= "Voted by" %> </th>
    <th> <%= "Created" %>  </th>
    <th> <%= "Modified" %> </th>
  </tr>

<% @votes.each_with_index do |vote, i| %>
  <tr>
    <td> <%= i+1 %> </td>
    <td> <%= link_to vote.session.title , vote.session %></td>
    <td> <%= vote.presenter.name %> </td>
    <td class="nowrap" ><%= l vote.created_at %> </td>
    <td class="nowrap" ><%= l vote.updated_at %> </td>
  </tr>
<% end %>
</table>


<p class="alignright"><a href="mailto:sessions@xpday.net">problems or questions</a> </p> 

