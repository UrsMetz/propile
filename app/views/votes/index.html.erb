<div>
<h2>Summary</h2>
<table class="tablelist">
  <tr>
    <th></th>
    <th> <%= "Session" %>  </th>
    <th> <%= "Presenter(s)" %> </th>
    <th> <%= "#votes" %>  </th>
  </tr>

<% @sessions.each_with_index do |session, i| %>
  <tr>
    <td> <%= i+1 %> </td>
    <td> <%= link_to session.title , session %></td>
    <td> <%= session_presenter_names(session) %> </td>
    <td> <%= session.votes.size %> </td>
  </tr>
<% end %>
</table>
</div>

